{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4",
      "excludeFiles": "{*.md,.git/**,node_modules/**,vendor/**}"
    }
  },
  "routes": [
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))$",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.php?path=$1"
    },
    {
      "src": "/api",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}